{
  "name": "koishi-plugin-multi-bot-controller-chatluna-charon",
  "description": "Charon, the ancient Greek god of the River Styx, is configured to assign each Bot an independent ChatLuna persona (with unique presets and models), achieving persona isolation across multiple Bots.",
  "version": "0.1.1",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib",
    "dist"
  ],
  "license": "MIT",
  "keywords": [
    "chatbot",
    "koishi",
    "plugin",
    "chatluna",
    "multi-bot",
    "persona"
  ],
  "peerDependencies": {
    "@koishijs/plugin-server": "^3.0.0",
    "koishi": "^4.18.7",
    "koishi-plugin-chatluna": "^1.3.0",
    "koishi-plugin-multi-bot-controller": "^1.0.3"
  },
  "peerDependenciesMeta": {
    "@koishijs/plugin-server": {
      "optional": true
    },
    "@kotoko76/koishi-plugin-chatluna-character": {
      "optional": true
    },
    "koishi-plugin-chatluna-long-memory": {
      "optional": true
    }
  },
  "koishi": {
    "description": {
      "zh": "古希腊掌管冥河渡河的神，用于为每个 Bot 配置独立的 ChatLuna 人设（预设和模型），实现多 Bot 不同人设的隔离。"
    },
    "service": {
      "required": [
        "chatluna",
        "multi-bot-controller"
      ],
      "optional": [
        "chatluna_character"
      ]
    }
  }
}
